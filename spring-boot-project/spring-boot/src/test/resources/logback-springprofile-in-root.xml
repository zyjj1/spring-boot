<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%property{LOG_FILE} [%t] ${PID:-????} %c{1}: %m%n BOOTBOOT</pattern>
		</encoder>
	</appender>
	<root level="INFO">
		<springProfile name="profile">
			<appender-ref ref="CONSOLE" />
		</springProfile>
	</root>
</configuration>
